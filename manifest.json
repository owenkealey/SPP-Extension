{
    "manifest_version": 2,
    "name": "SPP New Tab Extension",
    "version": "0.0.1",
    "chrome_url_overrides" : {
        "newtab": "newtab.html"
    },
    "default_locale": "en",
    "description": "Overrides the newtab page to better suit SPP Students.",
    "browser_action": {
        "default_icon": "icon.png"
    },
    "background": {
        "scripts": ["js/background.js"],
        "persistent": false
    },
    "permissions": ["identity", "identity.email"]

}